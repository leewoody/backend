#!/bin/bash
# preinst script for open-xchange-eas-provisioning-sms
#
# see: dh_installdeb(1)

set -e

# summary of how this script can be called:
#        * <new-preinst> `install'
#        * <new-preinst> `install' <old-version>
#        * <new-preinst> `upgrade' <old-version>
#        * <old-preinst> `abort-upgrade' <new-version>
# for details, see http://www.debian.org/doc/debian-policy/ or
# the debian-policy package


case "$1" in
    install|upgrade)
    ;;

    abort-upgrade)
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

dpkg-maintscript-helper mv_conffile /opt/open-xchange/etc/groupware/mobile_configuration_action_sms.properties \
    /opt/open-xchange/etc/eas-provisioning-sms.properties 6.20.99 open-xchange-mobile-configuration-json-action-sms -- $1 \
    $(dpkg-query -W -f='${Config-Version}' open-xchange-mobile-configuration-json-action-sms)

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
